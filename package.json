{
  "name": "@ontopic/viz-components",
  "version": "1.0.0",
  "description": "Production-ready data visualization components for React using Observable Plot and Recharts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./plot": {
      "types": "./dist/plot/index.d.ts",
      "import": "./dist/plot/index.mjs",
      "require": "./dist/plot/index.js"
    },
    "./recharts": {
      "types": "./dist/recharts/index.d.ts",
      "import": "./dist/recharts/index.mjs",
      "require": "./dist/recharts/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'No linting configured yet'"
  },
  "keywords": [
    "data visualization",
    "react",
    "observable-plot",
    "recharts",
    "charts",
    "graphs",
    "components"
  ],
  "author": "Ontopic",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ontopic/viz-packages.git",
    "directory": "packages/components"
  },
  "peerDependencies": {
    "@observablehq/plot": "^0.6.0",
    "lucide-react": "^0.454.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "recharts": "^2.15.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    },
    "@observablehq/plot": {
      "optional": false
    },
    "recharts": {
      "optional": false
    },
    "lucide-react": {
      "optional": false
    }
  },
  "dependencies": {
    "@ontopic/viz-core": "git+https://github.com/ctzn-pub/ontopic-core.git#v1.0.0",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.5.5",
    "topojson-client": "^3.1.0"
  },
  "devDependencies": {
    "@observablehq/plot": "^0.6.17",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "lucide-react": "^0.454.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "2.15.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "sideEffects": false
}
